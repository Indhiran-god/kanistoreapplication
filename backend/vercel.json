{
    "version": 2,
    "name": "APP",
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node" 
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)", 
        "dest": "/index.js"  
      },
      {
        "src": "/(.*)", 
        "dest": "/index.js" 
      }
    ]
  }
  
